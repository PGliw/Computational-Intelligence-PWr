function [ A, S ] = AMUSE( X, N, przes )
    T = size(X,2);
    Xcent = bsxfun(@minus, X, mean(X,2));
    Cx = (1/T)* (Xcent * Xcent');
    [V, VAL] = eig(Cx);
    Vbar = V(:, 1:N);
    Z = (VAL^-0.5)*Vbar'*Xcent;
    Z1 = Z(:,1:T-przes);
    Z2 = Z(:, 1+przes:T);
    R = (1/(T-przes))*Z1*Z2';
    Rbar = (R+R')/2;
    [Vz, VALz] = 
    disp(Rbar);
    
